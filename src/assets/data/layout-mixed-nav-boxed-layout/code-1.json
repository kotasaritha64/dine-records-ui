import { AppSettings } from '../../../service/app-settings.service';@Component({  selector: 'layout-mixed-nav-boxed-layout',  templateUrl: './layout-mixed-nav-boxed-layout.html'})export class LayoutMixedNavBoxedLayoutPage {  constructor(private appSettings: AppSettings) {    this.appSettings.appTopNav = true;    this.appSettings.appBoxedLayout = true;    document.body.classList.add('app-with-bg');  }  ngOnDestroy() {    this.appSettings.appTopNav = false;    this.appSettings.appBoxedLayout = false;    document.body.classList.remove('app-with-bg');  }}