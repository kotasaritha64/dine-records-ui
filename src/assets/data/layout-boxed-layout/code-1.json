import { Component } from '@angular/core';import { AppSettings } from '../../../service/app-settings.service';@Component({  selector: 'layout-boxed-layout',  templateUrl: './layout-boxed-layout.html'})export class LayoutBoxedLayoutPage {  constructor(private appSettings: AppSettings) {    this.appSettings.appBoxedLayout = true;    document.body.classList.add('app-with-bg');  }  ngOnDestroy() {    this.appSettings.appBoxedLayout = false;    document.body.classList.remove('app-with-bg');  }}