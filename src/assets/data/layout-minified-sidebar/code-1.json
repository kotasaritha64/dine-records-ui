import { Component } from '@angular/core';import { AppSettings } from '../../../service/app-settings.service';@Component({  selector: 'layout-collapsed-sidebar',  templateUrl: './layout-collapsed-sidebar.html'})export class LayoutCollapsedSidebarPage {  constructor(private appSettings: AppSettings) {    this.appSettings.appSidebarMinified = true;  }  ngOnDestroy() {    this.appSettings.appSidebarMinified = false;  }}